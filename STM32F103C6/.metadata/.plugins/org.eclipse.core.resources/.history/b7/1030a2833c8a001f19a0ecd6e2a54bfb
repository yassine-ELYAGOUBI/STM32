#ifndef INC_LCD_I2C_H_
#define INC_LCD_I2C_H_


#include <string.h>

#define LCD_ADDR (0x27 << 1)

#define PIN_RS    (1 << 0)
#define PIN_EN    (1 << 2)
#define BACKLIGHT (1 << 3)

#define LCD_DELAY_MS 5




#endif /* INC_LCD_I2C_H_ */
